<html>
<head>
  <script>
    function changeImage() {
      document.getElementById("image").src = "bird.jpeg";
      document.getElementById("text").innerHTML = "bird";
    }
  </script>
</head>
<body>
  <div onmouseover="changeImage()">
    <img id="image" src="photo.jpeg">
    <p id="text">photo</p>
  </div>
</body>
</html>

